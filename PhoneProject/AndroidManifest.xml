<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.phoneproject"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="18" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.phoneproject.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.phoneproject.Home"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.phoneproject.Settings"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.phoneproject.About"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.phoneproject.Help"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.phoneproject.NewProfile"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.phoneproject.New_location"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.phoneproject.Exist_location"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.phoneproject.Profile"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.phoneproject.ViewProfiles"
            android:label="@string/app_name" >
        </activity>
    </application>

</manifest>